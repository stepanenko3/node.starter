        "@types/express": "^4.16.1",
        "@types/underscore": "^1.8.9",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "compression": "^1.7.3",
        "convict": "^4.4.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "express-handlebars": "^3.0.0",
        "express-rate-limit": "^3.3.2",
        "express-validation": "^1.0.2",
        "express-ws": "^4.0.0",
        "handlebars": "^4.0.12",
